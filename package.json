{
  "name": "indice-ng-lib",
  "version": "0.0.8-beta.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-app": "ng serve app",
    "start-mock-server": "node mock-server.js",
    "build": "ng build --prod",
    "build-app": "ng build app --configuration=production ",
    "build-auth": "ng build ng-auth --configuration=production ",
    "build-components": "ng build ng-components --configuration=production",
    "postbuild-components": "ts-node ./projects/ng-components/scripts/css-bundle.ts",
    "test": "ng test",
    "lint": "ng lint",
    "build:prod": "cross-env NODE_ENV=production ng build --prod",
    "e2e": "ng e2e",
    "publish-components": "cd projects/ng-components && npm version patch && cd../.. && ng build ng-components --configuration=production && ts-node ./projects/ng-components/scripts/css-bundle.ts && cd dist/ng-components && npm publish",
    "publish-auth": "cd projects/ng-auth && npm version patch && cd../.. && ng build ng-auth --configuration=production && cd dist/ng-auth && npm publish",
    "publish-components-beta": "cd projects/ng-components && npm version prerelease --preid beta && cd../.. && ng build ng-components --configuration=production && ts-node ./projects/ng-components/scripts/css-bundle.ts && cd dist/ng-components && npm publish",
    "publish-auth-beta": "cd projects/ng-auth && npm version prerelease --preid beta && cd../.. && ng build ng-auth --configuration=production && cd dist/ng-auth && npm publish"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "14.0.0",
    "@angular/common": "14.0.0",
    "@angular/compiler": "14.0.0",
    "@angular/core": "14.0.0",
    "@angular/forms": "14.0.0",
    "@angular/platform-browser-dynamic": "14.0.0",
    "@angular/platform-browser": "14.0.0",
    "@angular/platform-server": "14.0.0",
    "@angular/router": "14.0.0",
    "@indice/ng-auth": "0.2.2",
    "@indice/ng-components": "0.2.157",
    "core-js": "^3.11.0",
    "oidc-client": "^1.11.5",
    "protractor": "~7.0.0",
    "rxjs": "^6.6.7",
    "ts-node": "~8.4.1",
    "tslint": "~6.1.3",
    "uuid": "8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^11.1.1",
    "@angular-devkit/build-angular": "14.0.0",
    "@angular/cli": "14.0.0",
    "@angular/compiler-cli": "14.0.0",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@tailwindcss/typography": "^0.4.0",
    "@types/fs-extra": "^9.0.11",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/uuid": "8.3.4",
    "autoprefixer": "^10.4.7",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.3",
    "jasmine-core": "~4.1.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine-html-reporter": "2.0.0",
    "karma-jasmine": "5.0.0",
    "karma": "~6.3.2",
    "ng-packagr": "^14.0.0",
    "oauth2-mock-server": "^4.0.0",
    "oidc-client": "^1.11.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^5.3.0",
    "postcss-scss": "^3.0.5",
