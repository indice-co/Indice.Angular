{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-auth": {
      "projectType": "library",
      "root": "projects/ng-auth",
      "sourceRoot": "projects/ng-auth/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ng-auth/tsconfig.lib.json",
            "project": "projects/ng-auth/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-auth/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-auth/src/test.ts",
            "tsConfig": "projects/ng-auth/tsconfig.spec.json",
            "karmaConfig": "projects/ng-auth/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-auth/tsconfig.lib.json",
              "projects/ng-auth/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ng-components": {
      "projectType": "library",
      "root": "projects/ng-components",
      "sourceRoot": "projects/ng-components/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ng-components/tsconfig.lib.json",
            "project": "projects/ng-components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-components/tsconfig.lib.prod.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-components/src/test.ts",
            "tsConfig": "projects/ng-components/tsconfig.spec.json",
            "karmaConfig": "projects/ng-components/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-components/tsconfig.lib.json",
              "projects/ng-components/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "demo-app": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/demo-app",
      "sourceRoot": "projects/demo-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./projects/demo-app/webpack.config.js"
            },
            "outputPath": "dist/demo-app",
            "index": "projects/demo-app/src/index.html",
            "main": "projects/demo-app/src/main.ts",
            "polyfills": "projects/demo-app/src/polyfills.ts",
            "tsConfig": "projects/demo-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/demo-app/src/favicon.ico",
              "projects/demo-app/src/assets"
            ],
            "styles": [
              "projects/demo-app/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/demo-app/src/environments/environment.ts",
                  "with": "projects/demo-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "demo-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-app:build"
          }
        },
        "test": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "main": "projects/demo-app/src/test.ts",
            "polyfills": "projects/demo-app/src/polyfills.ts",
            "tsConfig": "projects/demo-app/tsconfig.spec.json",
            "karmaConfig": "projects/demo-app/karma.conf.js",
            "assets": [
              "projects/demo-app/src/favicon.ico",
              "projects/demo-app/src/assets"
            ],
            "styles": [
              "projects/demo-app/src/styles.css"
            ],
            "customWebpackConfig": {
              "path": "projects/demo-app/src/webpack.config.js"
            },
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/demo-app/tsconfig.app.json",
              "projects/demo-app/tsconfig.spec.json",
              "projects/demo-app/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/demo-app/e2e/protractor.conf.js",
            "devServerTarget": "demo-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo-app:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "ng-auth",
  "cli": {
    "analytics": "fbb03e04-4116-41fe-aa03-eeccfcc4151e"
  }
}
